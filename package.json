{
  "name": "maple-js",
  "author": "Maple Labs",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build-typechain": "ts-node ./scripts/build-typechain.ts",
    "build-dist": "ts-node ./scripts/build-dist.ts",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "mock": "ts-node -r esm mockRunSDK.ts"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 110,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/maple-js.esm.js",
  "size-limit": [
    {
      "path": "dist/maple-js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/maple-js.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@maplelabs/LoanV1": "npm:@maplelabs/loan@v1.0.0",
    "@maplelabs/LoanV2": "npm:@maplelabs/loan@v2.0.0",
    "@maplelabs/debtLockerV1": "npm:@maplelabs/debt-locker@v1.0.0",
    "@maplelabs/debtLockerV2": "npm:@maplelabs/debt-locker@v2.0.0",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@types/node": "^17.0.12",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@typechain/ethers-v5": "^9.0.0",
    "esm": "^3.2.25",
    "ethers": "^5.5.3",
    "shelljs": "^0.8.5",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0"
  }
}
