{
  "version": "1.0.0",
  "license": "MIT",
  "name": "maple-js",
  "description": "Maple SDK for JavaScript",
  "author": "Maple Labs",
  "main": "dist/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-typechain": "ts-node ./scripts/build-typechain.ts",
    "build-dist": "ts-node ./scripts/build-dist.ts",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet",
    "lint:fix": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "format": "prettier --write '*/**/*.ts'"
  },
  "prettier": {
    "printWidth": 110,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@maplelabs/LoanV1": "npm:@maplelabs/loan@v1.0.0",
    "@maplelabs/LoanV2": "npm:@maplelabs/loan@v2.0.0",
    "@maplelabs/debtLockerV1": "npm:@maplelabs/debt-locker@v1.0.0",
    "@maplelabs/debtLockerV2": "npm:@maplelabs/debt-locker@v2.0.0",
    "@typechain/ethers-v5": "^9.0.0",
    "ethers": "^5.5.3",
    "shelljs": "^0.8.5",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0"
  }
}
