{
  "version": "0.0.1",
  "license": "MIT",
  "name": "maple-js",
  "description": "Maple SDK for JavaScript",
  "author": "Maple Labs",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rm -rf dist/ && rollup -c",
    "build-typechain": "node ./scripts/build-typechain.js",
    "test": "jest --collectCoverage",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet",
    "lint:fix": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "format": "prettier --write '*/**/*.ts'",
    "postinstall": "yarn build-typechain"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "jest": "^27.5.0",
    "rollup": "^2.52.8",
    "rollup-plugin-dts": "^4.1.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@maplelabs/LoanV1": "npm:@maplelabs/loan@v1.0.0",
    "@maplelabs/LoanV2": "npm:@maplelabs/loan@v2.0.0",
    "@maplelabs/debtLockerV1": "npm:@maplelabs/debt-locker@v1.0.0",
    "@maplelabs/debtLockerV2": "npm:@maplelabs/debt-locker@v2.0.0",
    "@typechain/ethers-v5": "^9.0.0",
    "ethers": "^5.5.3",
    "shelljs": "^0.8.5",
    "typechain": "^7.0.0"
  }
}
