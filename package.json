{
  "name": "@maplelabs/maple-js",
  "version": "0.0.62",
  "description": "Maple SDK for JavaScript",
  "author": "Maple Labs",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/maple-labs/maple-js"
  },
  "bugs": {
    "url": "https://github.com/maple-labs/maple-js/issues"
  },
  "homepage": "https://github.com/maple-labs/maple-js",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "scripts/",
    "config.json"
  ],
  "engines": {
    "node": ">=16.4.2"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rm -rf dist/ && rollup -c && cp -R src/abis dist/abis",
    "build-typechain": "node ./scripts/build-typechain.js",
    "build-addresses": "node ./scripts/build-addresses.js",
    "test": "jest --collectCoverage",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet",
    "lint:fix": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "format": "prettier --write '*/**/*.ts'",
    "postinstall": "yarn build-typechain",
    "rebuild": "yarn unlink && yarn build && yarn link"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "jest": "^27.5.0",
    "rollup": "^2.52.8",
    "rollup-plugin-dts": "^4.1.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@maplelabs/collateralLocker": "npm:@maplelabs/collateral-locker@v1.0.0",
    "@maplelabs/debtLockerV1": "npm:@maplelabs/debt-locker@v1.0.0",
    "@maplelabs/debtLockerV2": "npm:@maplelabs/debt-locker@v2.0.0",
    "@maplelabs/debtLockerV3": "npm:@maplelabs/debt-locker@v3.0.0",
    "@maplelabs/debtLockerV4": "npm:@maplelabs/debt-locker@4.0.0-rc.0",
    "@maplelabs/environmentMocks": "npm:@maplelabs/environment-mocks@v2.0.0-beta.0",
    "@maplelabs/fundingLocker": "npm:@maplelabs/funding-locker@v1.0.0",
    "@maplelabs/lateFeeCalculator": "npm:@maplelabs/late-fee-calculator@v1.0.0",
    "@maplelabs/liquidationsV2": "npm:@maplelabs/liquidations@2.0.0-rc.0",
    "@maplelabs/liquidityLocker": "npm:@maplelabs/liquidity-locker@v1.0.0",
    "@maplelabs/loanV1": "npm:@maplelabs/loan@v1.0.0",
    "@maplelabs/loanV2": "npm:@maplelabs/loan@v2.0.0",
    "@maplelabs/loanV3": "npm:@maplelabs/loan@3.0.0",
    "@maplelabs/loanV301": "npm:@maplelabs/loan@3.0.1-rc.0",
    "@maplelabs/loanV4": "npm:@maplelabs/loan@4.0.0-rc.0",
    "@maplelabs/mapleGlobals": "npm:@maplelabs/globals@v1.0.0",
    "@maplelabs/mapleGlobalsV2": "npm:@maplelabs/globals-v2@1.0.0-rc.0",
    "@maplelabs/mapleToken": "npm:@maplelabs/maple-token@1.0.0 ",
    "@maplelabs/mplRewards": "npm:@maplelabs/mpl-rewards@v1.0.0",
    "@maplelabs/pool": "npm:@maplelabs/pool@v1.0.0",
    "@maplelabs/poolV2": "npm:@maplelabs/pool-v2@1.0.0-rc.1.1",
    "@maplelabs/premiumCalculator": "npm:@maplelabs/premium-calculator@v1.0.0",
    "@maplelabs/repaymentCalculator": "npm:@maplelabs/repayment-calculator@v1.0.0",
    "@maplelabs/stakeLocker": "npm:@maplelabs/stake-locker@v1.0.0",
    "@maplelabs/util": "npm:@maplelabs/util@v1.0.0",
    "@maplelabs/withdrawalManager": "npm:@maplelabs/withdrawal-manager@1.0.0-rc.0",
    "@maplelabs/xmpl": "npm:@maplelabs/xmpl@v1.0.1",
    "@typechain/ethers-v5": "^9.0.0",
    "ethers": "^5.5.3",
    "shelljs": "^0.8.5",
    "typechain": "^7.0.0"
  }
}
